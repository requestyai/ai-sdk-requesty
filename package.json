{
    "name": "@requesty/ai-sdk",
    "version": "2.0.4",
    "license": "Apache-2.0",
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": ["dist/**/*"],
    "scripts": {
        "build": "tsdown",
        "clean": "rm -rf dist && rm -rf internal/dist",
        "dev": "tsdown --watch",
        "lint": "biome lint",
        "typecheck": "tsc --noEmit",
        "stylecheck": "biome check --linter-enabled=false",
        "format": "biome check --linter-enabled=false --fix",
        "test": "npm run test:node && npm run test:edge",
        "test:all": "npm run test && npm run test:e2e",
        "test:edge": "vitest --config vitest.edge.config.ts --run",
        "test:node": "vitest --config vitest.node.config.ts --run",
        "test:edge:watch": "vitest --config vitest.edge.config.ts --watch",
        "test:node:watch": "vitest --config vitest.node.config.ts --watch",
        "test:e2e": "vitest --config vitest.e2e.config.ts --run",
        "test:e2e:watch": "vitest --config vitest.e2e.config.ts --watch",
        "test:pizza": "vitest --config vitest.e2e.config.ts --run src/e2e/pizza.test.ts",
        "test:pizza:watch": "vitest --config vitest.e2e.config.ts --watch src/e2e/pizza.test.ts"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./internal": {
            "types": "./dist/internal/index.d.ts",
            "import": "./dist/internal/index.mjs",
            "module": "./dist/internal/index.mjs",
            "require": "./dist/internal/index.js"
        }
    },
    "dependencies": {
        "@ai-sdk/provider": "3.0.0-beta.8",
        "@ai-sdk/provider-utils": "^3.0.9"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.5",
        "@edge-runtime/vm": "5.0.0",
        "@types/jest": "29.5.14",
        "@types/node": "22.13.5",
        "ai": "6.0.0-beta.72",
        "dotenv": "^17.2.3",
        "msw": "^2.11.3",
        "tsdown": "^0.15.7",
        "typescript": "5.7.3",
        "vitest": "3.0.7",
        "zod": "^4.1.11"
    },
    "peerDependencies": {
        "vite": "^7.1.7",
        "zod": "^3.25.76 || ^4.0.0"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/requestyai/ai-sdk-requesty",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/requestyai/ai-sdk-requesty"
    },
    "bugs": {
        "url": "https://github.com/requestyai/ai-sdk-requesty/issues"
    },
    "keywords": ["ai"],
    "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
